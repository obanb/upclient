{
  "name": "up-frontend",
  "version": "0.0.1-alpha.1",
  "description": "Frontend for UP",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "generate:lang": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} ./scripts/default-lang.ts",
    "build": "npm run generate:lang && next build && tsc --project tsconfig.server.json",
    "dev:simple": "npm run generate:lang && tsc --project tsconfig.server.json && node --max_old_space_size=4096 dist/index.js",
    "heroku-postbuild": "npm run build",
    "start": "NODE_ENV=production node dist/index.js",
    "eslint": "eslint client --ext ts,tsx",
    "prettier:show": "prettier --list-different \"./**/*\"",
    "prettier:write": "prettier \"./**/*\" --write",
    "test": "jest --verbose --colors --runInBand",
    "coverage": "jest --coverage --runInBand",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pronovaso/up-frontend.git"
  },
  "author": "UP - Pronovaso",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pronovaso/up-frontend/issues"
  },
  "homepage": "https://github.com/pronovaso/up-frontend#readme",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "client/**/*.ts",
      "server/**/*.ts"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "4.2.1",
    "@material-ui/styles": "^4.3.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "latest",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "apollo-upload-client": "^11.0.0",
    "axios": "0.19.0",
    "body-parser": "^1.19.0",
    "classnames": "2.2.6",
    "cookie-parser": "1.4.4",
    "crypto": "1.0.1",
    "dotenv": "^8.1.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "express": "^4.17.1",
    "express-http-proxy": "1.5.1",
    "formik": "^1.5.8",
    "graphql": "^14.5.1",
    "graphql-tag": "2.10.1",
    "intl": "1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.15",
    "moment": "2.24.0",
    "next": "^9.0.4",
    "next-transpile-modules": "^2.3.1",
    "react": "^16.9.0",
    "react-apollo": "^3.0.1",
    "react-dom": "^16.9.0",
    "react-intl": "2.9.0",
    "ts-node": "^8.3.0",
    "up-graphql": "git+https://d1a9c4f711b851ee9af72e86b14e98947c0725c3:x-oauth-basic@github.com/pronovaso/up-graphql.git",
    "up-validations": "git+https://d1a9c4f711b851ee9af72e86b14e98947c0725c3:x-oauth-basic@github.com/pronovaso/up-validations.git"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@playlyfe/gql": "^2.6.2",
    "@types/apollo-upload-client": "8.1.2",
    "@types/babel__core": "7.1.2",
    "@types/body-parser": "^1.17.1",
    "@types/classnames": "^2.2.9",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "6.1.1",
    "@types/eslint": "^6.1.1",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/express": "^4.17.1",
    "@types/express-http-proxy": "^1.5.12",
    "@types/graphql": "^14.5.0",
    "@types/intl": "1.2.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/js-cookie": "^2.2.2",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/lodash": "^4.14.137",
    "@types/prettier": "^1.18.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-intl": "^3.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-intl": "^4.1.14",
    "concurrently": "^4.1.2",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
